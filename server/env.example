ENV=dev
LOG_LEVEL=INFO

# Database
DATABASE_URL=postgresql+psycopg2://postgres:postgres@localhost:5432/slava_licensing

# Redis
REDIS_URL=redis://localhost:6379/0

# JWT keys paths
JWT_PRIVATE_KEY_PATH=server/keys/jwtRS256.key
JWT_PUBLIC_KEY_PATH=server/keys/jwtRS256.key.pub
JWT_EXPIRE_MINUTES=10080
JWT_ISSUER=slava-licensing

# Business
RESERVATION_TTL_MINUTES=15

# Rate limits
RATE_LIMIT_PER_TOKEN_RPS=20
RATE_LIMIT_PER_IP_RPS=60

# Metrics
METRICS_ENABLED=true


